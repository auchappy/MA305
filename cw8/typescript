]0;piers@Lenovoman-G50-45: ~/Documents/MA305/cw8piers@Lenovoman-G50-45:~/Documents/MA305/cw8$ vim[K[K[Kcat cwa[K8a.py
#!/usr/bin/env python 
from myfuns import f
from  mySums import *


a=0.0
b=1.0 
n=int(input('Enter the number of rectangles n:'))

exact=2

for i in range(5,n):
    N=2**i
    approx1=lsum(f,a,b,N)
    approx2=rsum(f,a,b,N)
    approx3=msum(f,a,b,N)

    error1=abs(approx1-exact)
    error2=abs(approx2-exact)
    error3=abs(approx3-exact)

    print("N=",N)
    print("                 Approximation       Error")
    print("  Left-Sum: \t {0:12.10f}\t {1:12.10f}".format(approx1, error1))
    print(" Right-Sum: \t {0:12.10f}\t {1:12.10f}".format(approx2, error2))
    print("   Mid-Sum: \t {0:12.10f}\t {1:12.10f}".format(approx3, error3))
    print()



]0;piers@Lenovoman-G50-45: ~/Documents/MA305/cw8piers@Lenovoman-G50-45:~/Documents/MA305/cw8$ cat mys[KSums.py
#!/user/bin/env python
'''
==============================================================
MA305 - CW8 : Piers Chapman - 29OCT18
Purpose: To determine the riemans sum of a function with relation to the number of 'boxes' utilised.
==============================================================
'''

def lsum(f,a,b,n):
	h = (b-a)/n
	left_sum = 0
	for i in range(n):
		xi = a+i*h
		fi=f(xi)
		print('{0:3d}    {1:10.5f}   {2:10.5f}'.format(i,xi,fi))
		left_sum += h*fi
	return left_sum

def msum(f,a,b,n):
	h = (b-a)/n
	mid_sum = 0
	for i in range(n):
		xi = a+0.5*h+i*h
		fi=f(xi)
		print('{0:3d}    {1:10.5f}   {2:10.5f}'.format(i,xi,fi))
		mid_sum += h*fi
	return mid_sum

def rsum(f,a,b,n):
	h = (b-a)/n
	right_sum = 0
	for i in range(1,n+1):
		xi = a+i*h
		fi=f(xi)
		print('{0:3d}    {1:10.5f}   {2:10.5f}'.format(i,xi,fi))
		right_sum += h*fi
	return right_sum
